    <%--    MODAL para cambio de Contraseña   --%>
    <div class="modal fade" id="modalChangePass" role="dialog">
        <div class="modal-dialog modal-dialog-scrollable modal-sm" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Cambiar contraseña</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body" style="padding:10px;">
                    <label>Ingrese su contraseña</label>
                    <input id="txtOldPass" type="password" placeholder="Contraseña actual" class="form-control input_pass" />
                    <hr />
                    <div style="margin: 10px 0;" >
                        <label>Ingrese su nueva contraseña</label>
                        <input id="txtNewPass" type="password" placeholder="Nueva contraseña" class="form-control input_pass" />
                    </div>
                    
                    <div style="margin: 10px 0;" >
                        <label>Confirme su nueva contraseña</label>
                        <input id="txtNewPassR" type="password" placeholder="Confirmar contraseña" class="form-control input_pass" />
                    </div>
                    
                </div>
                <div class="modal-footer">
                    <button type="button" id="btnModalChangePassAccept" class="btn btn-primary">Cambiar</button>
                    <button type="button" id="btnModalChangePassCancel" class="btn btn-primary" data-dismiss="modal" >Cancelar</button>
                </div>
            </div>
        </div>
    </div>

