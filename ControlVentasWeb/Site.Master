    <script>
        $(document).ready(function () {
            var baseUrl = '/Account/Login.aspx';

            $('#hrefCambioPass').click(function () {
                console.log("show modal");
                $('#modalChangePass').modal('show');
            });

            $('#btnSwitchProfile').click(function () {
                ComboBoxPerfiles.PerformCallback();
                console.log(ComboBoxPerfiles);
                console.log("show modal");
                $('#modalChangePerfil').modal('show');
            });

            $('#hrefSoporte').click(function () {
                var URL = '/web/ConfiguracionGeneral.aspx' + '/GetInfoSoporte';

                api_service.postWithParams(URL, {}, function (response) {
                    if (response.d.Status == 200) {

                        $('#modalSoporteInfo').modal('show');
                        $('#txtSoporteURL').val(response.d.Data[0]);
                        $('#txtSoporteTel').val(response.d.Data[1]);
                        $('#txtSoporteResp').val(response.d.Data[2]);

                        //bootstrap_alert.success(response.d.Message);
                    } else {
                        bootstrap_alert.error(response.d.Message);
                        console.log(response.d.ErrorMessage);
                    }
                });
            });

            $('#btnModalChangePassAccept').click(function () {
                var URL = baseUrl + '/Changepass';
                var OldPass = $('#txtOldPass').val()
                var NewPass = $('#txtNewPass').val()
                var NewPassR = $('#txtNewPassR').val()

                if (OldPass != "" || NewPass != "" || NewPassR != "") {
                    if (NewPass == NewPassR) {
                        //console.log(Email);
                        api_service.postWithParams(URL, { CurrentPass: OldPass, NewPass: NewPass, NewPassR: NewPassR }, function (response) {
                            if (response.d.Status == 200) {
                                $('#modalChangePass').modal('hide');
                                bootstrap_alert.success(response.d.Message);
                            } else if (response.d.Status == 400) {
                                bootstrap_alert.warning(response.d.Message);
                            } else if (response.d.Status == 406) {
                                bootstrap_alert.info(response.d.Message);
                            } else {
                                bootstrap_alert.error(response.d.Message);
                                console.log(response.d.ErrorMessage);
                            }
                        });
                    } else {
                        bootstrap_alert.info("La nueva contraseña no coincide con la contraseña de confirmación");
                    }
                } else
                    bootstrap_alert.info("Faltan campos por llenar");
            });

            GetMessageOnInit();

            function GetMessageOnInit() {
                var URL = baseUrl + '/HasMessageOninit';
                api_service.post(URL, {}, function (response) {
                    if (response.d.Status == 302) {
                        bootstrap_alert.info(response.d.Message);
                    }
                });
            }

            // *********** Mensajes del systema ************************
            var Refreshdelay = 2000;

            function GetMessageSystem() {
                var URL = '/web/Webpro.aspx' + '/HasMessageSystem';
                api_service.postsys(URL, {}, function (response) {
                    if (response.d.Status == 200) {
                        ShowMessageBoxSystem(response.d.Data);
                    } else {
                        console.log(response.d.ErrorMessage);
                    }
                });
            }

            function ShowMessageBoxSystem(Message) {
                try {
                    console.log(Message);
                    if (Message.Notify !== '') {
                        bootstrap_alert.info(Message.Notify);
                    }

                    if (Message.Title !== '' || Message.Text !== '') {
                        MessageBoxSys.Show(true, Message.Title, Message.Text);
                        return;
                    }

                    if (!Message.IsShow) {
                        MessageBoxSys.Show(false, Message.Title, Message.Text);
                    }
                } catch (e) {
                    console.log(e);
                }

            }

            //GetMessageSystem();
            //setInterval(GetMessageSystem, Refreshdelay);

            //myfunction();

            // *********** Mensajes del systema ************************

        });


    </script>
